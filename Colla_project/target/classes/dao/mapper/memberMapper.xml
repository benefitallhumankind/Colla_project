<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.MemberDao">
	<insert id="insertMember" parameterType="member">
		insert into test_member
		values(member_seq.nextval, #{mNum}, #{mId}, #{mPassword}, #{mName}, #{mEmail})
	</insert>
	<update id="updateMember" parameterType="member">
		update test_member
		set m_name = #{mName}, m_email = #{mEmail}
		where m_num = #{mNum}
	</update>
	<delete id="deleteMember">
		delete
		from test_memeber
		where m_num = #{mNum}
	</delete>
	<select id="selectMemberByNum" resultMap="memberMap">
		select
		from test_member
		where m_num = #{mNum}
	</select>
	<select id="selectMemberById" resultMap="memberMap">
		select
		from test_memeber
		where m_id = #{mId}
	</select>
	<select id="selectAllMember" resultMap="memberMap">
		select
		from test_memeber
	</select>
	<resultMap type="member" id="memberMap">
		<result property="mNum" column="m_num"/>
		<result property="mId" column="m_id"/>
		<result property="mPassword" column="m_password"/>
		<result property="mName" column="m_name"/>
		<result property="mEmail" column="m_email"/>
	</resultMap>
</mapper>
